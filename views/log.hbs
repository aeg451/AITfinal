<center><h2><u><b>Running Log</u></b></h2><br></center>
<div><center>
<table width = '90%' class="table table-bordered table-hover"></center>
<tr class="active">
  <th></th>
  <th>Date</th>
  <th>Type</th>
  <th>Distance</th>
  <th>Pace</th>
  <th>Splits</th>
  <th>Notes</th>
  <th>Location</th>
</tr>
{{#each log as |logged|}}
<form method='POST' action="">
  <tr>
    <td><input type='checkbox' name='delete' value='delete'>&nbsp;&nbsp;<input type='submit' value='Delete'><input type='hidden' name='id' value='{{logged._id}}'></td>
    <td>{{logged.date}}</td>
    <td>{{logged.type}}</td>
    <td>{{logged.distance}}</td>
    <td>{{logged.pace}}</td>
    <td>{{logged.splits}}</td>
    <td>{{logged.notes}}</td>
    <td>{{logged.location}}</td>
  </tr>
</form>
{{/each}}
<center></table></center>
</div> 

