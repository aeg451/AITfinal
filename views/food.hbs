{{#each food as |foods|}}
<form method='POST' action="">
  <tr>
    <td><input type='checkbox' name='delete' value='delete'>&nbsp;&nbsp;<input type='submit' value='Delete'><input type='hidden' name='id' value='{{foods._id}}'></td>
    <td>{{foods.date}}</td>
    <td>{{foods.breakfast}}</td>
    <td>{{foods.lunch}}</td>
    <td>{{foods.dinner}}</td>
    <td>{{foods.snack}}</td>
    <td>{{foods.other}}</td>
    <td>{{foods.exercise}}</td>
  </tr>
</form>
{{/each}}
